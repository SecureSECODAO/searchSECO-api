{
  "name": "searchseco-api",
  "version": "1.0.0",
  "description": "A bare-bones API around the SearchSECO functionalities.",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve",
    "build": "npm run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js",
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "gen-js": "npx antlr4ts-cli -o ./src/parser/languages/javascript/lib -visitor ./src/parser/languages/javascript/grammars/JavaScriptLexer.g4 && npx antlr4ts-cli -o ./src/parser/languages/javascript/lib -visitor ./src/parser/languages/javascript/grammars/JavaScriptParser.g4",
    "gen-py3": "npx antlr4ts-cli -o ./src/parser/languages/python3/lib -visitor ./src/parser/languages/python3/grammars/Python3.g4"
  },
  "author": "Wouter Beffes",
  "license": "MIT",
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "async-lock": "^1.4.0",
    "babel-cli": "^6.26.0",
    "body-parser": "^1.20.2",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "decompress-zip": "^0.3.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jszip": "^3.10.1",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "node": "^19.8.1",
    "octokit": "^2.0.14",
    "path": "^0.12.7",
    "request-promise": "^4.2.6",
    "unzip-stream": "^0.3.1",
    "uuid": "^9.0.0",
    "viem": "^0.3.17"
  },
  "devDependencies": {
    "@types/antlr4": "^4.11.2",
    "@types/async-lock": "^1.4.0",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/request-promise": "^4.1.48",
    "@types/unzip-stream": "^0.3.1",
    "@types/uuid": "^9.0.1",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "concurrently": "^8.0.1",
    "express-winston": "^4.2.0",
    "nodemon": "^2.0.22",
    "winston": "^3.8.2"
  }
}
